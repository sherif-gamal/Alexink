<script>
  $('#ajax-content a').on('click', function(e) {
      e.preventDefault();
      if ($(this).hasClass('add-full')) {
        $('#content').addClass('full-content');
      }
      else {
        $('#content').removeClass('full-content');
      }
      var url = $(this).attr('href');
      window.location.hash = url;
      console.log(url);
      LoadAjaxContent(url);
  });
</script>